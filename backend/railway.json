{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "healthcheckInterval": 30
  },
  "variables": {
    "PORT": "3000",
    "NODE_ENV": "production",
    "LOG_LEVEL": "info"
  }
}